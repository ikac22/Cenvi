# call from excercise directory
TO_MAIN=<to_main>
BUILD_DIR = build
PROGRAM = program-name
CPP_FILES = $(TO_MAIN)src/main.cpp src/imp.cpp 
# add new files
CPP_FILES += 

CPPC = g++ 
CPPC_FLAGS = -Iinc -I$(TO_MAIN)inc


$(BUILD_DIR)/$(PROGRAM).elf: $(CPP_FILES)
	$(CPPC) $(CPPC_FLAGS) -o $(@) $(CPP_FILES)
